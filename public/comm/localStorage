01unavigator,location,document,e,f,gvar d=/MSIE\s+[2-9]/.test(e.userAgent),b,c=this.globalStorage;if(c)b=c[f.hostname];else if(d){var a=g.documentElement;b={setItem:function(b,c){a.setAttribute('value',c),a.save(b)},getItem:function(b){return a.load(b),a.getAttribute('value')},removeItem:function(b){a.removeAttribute('value'),a.save(b)}},a.addBehavior('#default#userdata')}else b={setItem:function(){},getItem:function(){return null},removeItem:function(){}};return b