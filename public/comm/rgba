010color,parseInt,c,bfunction a(d,e,f,g){switch(arguments.length){case 1:switch(typeof d){case'number':return'rgba('+[d>>>24&255,d>>>16&255,d>>>8&255,(d&255)/255]+')';case'array':return a.apply(null,d);case'string':if(/^([\da-f]{3}){1,2}$/i.test(d))return'#'+c;if(/^#?([\da-f]{4}){1,2}$/i.test(d))switch(d.length){case 5:d=d.slice(1);case 4:return a.apply(null,d.split('').map(function(a){return b(a+a,16)}));case 9:d=d.slice(1);case 8:return a(b(g,16))}return c}case 2:e>1&&(e/=256);if(/^#?([\da-f]{3}){1,2}[\da-f]{1,2}$/i.test(d))return d=d.replace(/^#/,'').split('').map(function(a){return b(a.length==1?a+a:a,16)}).slice(0,3),d.push(e),'rgba('+d+')';if(typeof d==='number')return'rgba('+[d>>>16&255,d>>>8&255,d&255,e]+')';break;case 3:return'rgb('+[d,e,f]+')';case 4:return'rgba('+[d,e,f,g]+')'}return null}return a